{
  "approach": "maml",
  "dataset": "mnist",
  "n_way": 5,
  "k_shot": 1,
  "q_query": 15,
  "base_filters": 32,
  "depth": 4,
  "kernel_size": 3,
  "dropout": 0.0,
  "use_bn": true,
  "inner_steps": 1,
  "second_order": false,
  "inner_lr": 0.4,
  "meta_lr": 0.001,
  "episodes_per_epoch": 200,
  "epochs": 10,
  "batch_size_episodes": 4,
  "lopt_hidden": 32,
  "lopt_lstm": false,
  "do_ablate": false,
  "inner_steps_sweep": "1,2",
  "support_sweep": "1,5",
  "query_sweep": "5,15",
  "seed": 42,
  "data_dir": "./data",
  "out_dir": "./results_unified",
  "device": "cpu"
}